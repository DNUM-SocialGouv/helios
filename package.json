{
  "name": "helios",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "amibroken": "yarn deadcode && yarn lint && yarn typecheck && yarn test && yarn build",
    "bash:production": "scalingo -a helios run bash",
    "bash:production:2xl": "scalingo -a helios run --size 2XL bash",
    "build": "next build",
    "deadcode": "ts-prune --ignore 'src/pages'",
    "depcheck": "depcheck",
    "dev": "yarn start:db && next dev",
    "lint": "yarn lint:typescript && yarn lint:python",
    "lint:typescript": "npx eslint src database datacrawler",
    "lint:typescript:fix": "yarn lint:typescript --fix",
    "lint:python": "pipenv run lint",
    "lint:python:fix": "pipenv run format",
    "lint:scss": "stylelint '**/*.module.css'",
    "lint:scss:fix": "yarn lint:scss --fix",
    "migrations": "ts-node -T ./database/migrations.ts",
    "psql:local": "docker-compose up -d postgres && docker exec -it helios_db psql -U helios helios",
    "psql:production": "scalingo -a helios pgsql-console",
    "postinstall": "husky install",
    "populateDatabase": "yarn retrieveFiness && yarn updateEJ && yarn updateET && yarn updateAutorisationsMédicoSocial && yarn updateAutorisationsSanitaire",
    "populateDatabase:local": "yarn retrieveFiness && yarn updateEJ && yarn updateET && yarn updateActivitésMédicoSociaux:local && yarn updateActivitésSanitaire:local && yarn updateAutorisationsMédicoSocial:local && yarn updateAutorisationsSanitaire:local",
    "retrieveFiness": "ts-node -T datacrawler/legacy/infrastructure/controllers/téléchargeLesDonnéesBrutesDeFinessCron.ts",
    "updateEJ": "TS_NODE_TRANSPILE_ONLY=true node --loader ts-node/esm --max-old-space-size=8192 datacrawler/legacy/infrastructure/controllers/metsÀJourLesEntitésJuridiquesCron.ts",
    "updateET": "TS_NODE_TRANSPILE_ONLY=true node --loader ts-node/esm --max-old-space-size=8192 datacrawler/legacy/infrastructure/controllers/metsÀJourLesÉtablissementsTerritoriauxCron.ts",
    "updateActivitésMédicoSociaux:local": "pipenv run populate_activite_et_medico_social",
    "updateActivitésSanitaire:local": "pipenv run populate_activite_et_sanitaire",
    "updateAutorisationsMédicoSocial": "python -m datacrawler.ajoute_les_autorisations_des_établissements_médico_sociaux",
    "updateAutorisationsMédicoSocial:local": "pipenv run populate_autorisation_et_capacite_medico_social",
    "updateAutorisationsSanitaire": "python -m datacrawler.ajoute_les_autorisations_des_établissements_sanitaires",
    "updateAutorisationsSanitaire:local": "pipenv run populate_autorisation_et_capacite_sanitaire",
    "start": "next start",
    "start:db": "docker-compose down && docker-compose up -d && while ! docker-compose logs | grep -q 'listening on IPv4 address'; do echo 'Waiting for database'; sleep 0.5; done; yarn migrations",
    "start:production": "ts-node -T src/backend/infrastructure/controllers/allumeLaProductionCron.ts",
    "stop:production": "ts-node -T src/backend/infrastructure/controllers/éteintLaProductionCron.ts",
    "test": "yarn test:typescript && yarn test:python",
    "test:accessibility": "lhci autorun",
    "test:typescript": "NODE_ENV=test yarn migrations && jest --runInBand",
    "test:typescript:watch": "yarn test:typescript --watch",
    "test:typescript:coverage": "yarn test:typescript --coverage",
    "test:python": "NODE_ENV=test yarn migrations && pipenv run test",
    "typecheck": "yarn typecheck:typescript && yarn typecheck:python",
    "typecheck:typescript": "tsc",
    "typecheck:python": "pipenv run typecheck"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.4.1",
    "@sentry/nextjs": "^7.1.1",
    "chart.js": "^3.8.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "dotenv-defaults": "^5.0.0",
    "next": "12.2.3",
    "node-fetch": "2.6.7",
    "pg": "^8.7.3",
    "react": "18.0.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "18.0.0",
    "ssh2": "^1.9.0",
    "ssh2-sftp-client": "^8.0.0",
    "typeorm": "^0.3.6",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@lhci/cli": "^0.9.0",
    "@swc/core": "^1.2.168",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.23",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/ssh2": "^0.5.52",
    "@types/ssh2-sftp-client": "^7.0.1",
    "@types/xml-js": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "depcheck": "^1.4.3",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-testing-library": "^5.3.0",
    "husky": "^7.0.4",
    "jest": "^28.0.2",
    "jest-environment-jsdom": "^28.0.2",
    "jest-when": "^3.5.1",
    "next-router-mock": "^0.7.4",
    "stylelint": "^14.9.1",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.7.0",
    "ts-prune": "^0.10.3",
    "typescript": "4.6.3"
  },
  "resolutions": {
    "got": "^11.8.5"
  },
  "engines": {
    "node": "16.16"
  }
}
